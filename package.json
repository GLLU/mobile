    "start": "node_modules/react-native/packager/packager.sh",
    "eslint": "eslint",
    "test": "jest",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/com/infash/infash/src/main/res/",
    "gradle-build": "cd android && ./gradlew assembleRelease && cd ../",
    "pack-android": "npm run bundle-android && npm run gradle-build",
    "run-android": "react-native run-android",
    "run-ios": "react-native run-ios --scheme infash-debug",
    "storybook": "storybook start -p 7007",
    "code-push-ios": "code-push release-react infash ios --plistFile \"ios/gllu/Info.plist\" --outputDir ./sourceMap/ios",
    "code-push-android": "code-push release-react infash android --outputDir ./sourceMap/android"
  },
